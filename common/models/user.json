{
  "name": "User",
  "description": "Application user",
  "plural": "users",
  "base": "PersistedModel",
  "idInjection": true,
  "strict": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "String",
      "id": true,
      "required": true,
      "index": true
    },
    "email": {
      "type": "String",
      "required": true
    },
    "firstName": "String",
    "lastName": "String",
    "registrationDate": "Date",
    "type": {
      "type": "String",
      "required": true
    },
    "passwordHash": {
      "type": "String",
      "required": true
    },
    "permissions": "Object"
  }
}
